ting context\r\n  canvas.width = pixelCrop.width;\r\n  canvas.height = pixelCrop.height;\r\n\r\n  // paste generated rotate image with correct offsets for x,y crop values.\r\n  ctx.putImageData(\r\n    data,\r\n    Math.round(0 - safeArea / 2 + image.width * 0.5 - pixelCrop.x),\r\n    Math.round(0 - safeArea / 2 + image.height * 0.5 - pixelCrop.y)\r\n  );\r\n\r\n  // As Base64 string\r\n  // console.log(canvas.toDataURL(\"image/jpeg\"));\r\n  return canvas.toDataURL(\"image/jpeg\");\r\n\r\n  // As a blob\r\n  return new Promise((resolve) => {\r\n    canvas.toBlob((file) => {\r\n      resolve(URL.createObjectURL(file));\r\n    }, \"image/jpeg\");\r\n  });\r\n}\r\n",{"ruleId":"332","severity":1,"message":"333","line":3,"column":10,"nodeType":"334","messageId":"335","endLine":3,"endColumn":23},{"ruleId":"332","severity":1,"message":"336","line":5,"column":8,"nodeType":"334","messageId":"335","endLine":5,"endColumn":16},{"ruleId":"337","replacedBy":"338"},{"ruleId":"339","replacedBy":"340"},{"ruleId":"332","severity":1,"message":"341","line":2,"column":22,"nodeType":"334","messageId":"335","endLine":2,"endColumn":29},{"ruleId":"332","severity":1,"message":"342","line":2,"column":31,"nodeType":"334","messageId":"335","endLine":2,"endColumn":46},{"ruleId":"332","severity":1,"message":"343","line":1,"column":40,"nodeType":"334","messageId":"335","endLine":1,"endColumn":51},{"ruleId":"332","severity":1,"message":"344","line":2,"column":10,"nodeType":"334","messageId":"335","endLine":2,"endColumn":19},{"ruleId":"332","severity":1,"message":"345","line":3,"column":8,"nodeType":"334","messageId":"335","endLine":3,"endColumn":20},{"ruleId":"332","severity":1,"message":"346","line":6,"column":16,"nodeType":"334","messageId":"335","endLine":6,"endColumn":18},{"ruleId":"332","severity":1,"message":"347","line":10,"column":5,"nodeType":"334","messageId":"335","endLine":10,"endColumn":15},{"ruleId":"348","severity":1,"message":"349","line":7,"column":1,"nodeType":"350","endLine":19,